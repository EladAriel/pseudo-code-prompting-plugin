  "features": [
    "input_validation",
    "progress_tracking",
    "error_recovery",
    "rollback_support",
    "preference_persistence",
    "timeout_protection",
    "state_management",
    "checkpoint_recovery"
  ],
  "metadata": {
    "author": "Elad Ariel",
